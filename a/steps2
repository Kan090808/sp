while(havent read "END"){
	//cut off comments
	if(readline contain comments){
		if(readline have word before "."){
			cut by "." ;
			replace readline by the word before "."; 
		}else{
			readline = null;
		}
	}

	
	if(readline not null){
		cut to be token;
		if(token more then 3){
			let third token and fourth to be one token;
		}
		if(token not more than 3){
			
			if(prog havent start){
				if(have three token){
					if(second token is START){
						first token is progname;
						record prog started;
						third token is start address;
					}
				}
			}else if(prog started){
				if(cut array size== 1){
					if(token is RSUB){

					}else if(token is mnemonic){
						error:no operand;
					}else{
						error;
					}
				}else if(cut array size == 2){
					if(first token is mnemonic){

					}else if(first token is END){

					}else {
						error;
					}
				}else{
					if(first token is mnemonic){
						if(contains ","){

						}else {
							error;
						}
					}else if(second token is mnemonic{

					}
				}
			}
		
		}else{
			error:cut array size > 3;
		}
	}
}